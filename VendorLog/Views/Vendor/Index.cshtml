@{
  Layout = "Layout";
}

@using VendorLog.Models;

<h1>Vendors</h1>
<ul>
  @foreach(Vendor vendor in Model)
  {
    <li><a href="/vendor/@vendor.Id">@vendor.Name</a></li>
  }
</ul>
<form action="/vendor/new">
  <button type="submit">Add New Vendor</button>
</form>
<br>
<p><a href="/">HomePage</a></p>